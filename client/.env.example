# Client Environment Variables
# Copy this file to .env and update the values


# Backend API URL (production)
VITE_API_URL=https://gupshup-cafe.onrender.com

# Socket.io URL (production, usually same as API URL)
VITE_SOCKET_URL=https://gupshup-cafe.onrender.com

# Hugging Face API Key (optional, only if using AI topic generation with your own key)
# VITE_HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Enable/disable features
VITE_ENABLE_AI_TOPICS=true
VITE_ENABLE_AUDIO=true
